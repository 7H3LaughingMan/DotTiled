{
  "metadata": [
    {
      "src": [
        {
          "src": "../src",
          "files": [
            "DotTiled/DotTiled.csproj"
          ]
        }
      ],
      "dest": "api",
      "enumSortOrder": "declaringOrder"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern",
      "template"
    ],
    "globalMetadata": {
      "_appName": "DotTiled",
      "_appTitle": "DotTiled",
      "_enableSearch": true,
      "pdf": false
    }
  }
}